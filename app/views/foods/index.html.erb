<p style="color: green"><%= notice %></p>

<h1>Foods</h1>

<div id="foods">
   <%= render partial: '/shared/food' %>
  <% @foods.each do |food| %>
    <%= render food %>
    <p>
      <%= link_to "Show this food", food %>
    </p>

  <% end %>
</div>

<%= link_to "Add food", new_food_path, class: "navlink" %>
